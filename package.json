{
  "type": "module",
  "name": "@lwj/scenes",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jason Lengstorf <jason@lengstorf.com>",
  "license": "MIT",
  "scripts": {
    "dev": "rm -rf .cache && yarn build && http-server public",
    "build": "toast incremental .",
    "prepare": "breadbox --dest public/web_modules",
    "breadbox": "breadbox --dest public/web_modules",
    "serve": "http-server public"
  },
  "dependencies": {
    "@sector/breadbox": "^0.0.4",
    "@socket-studio/preact": "^1.0.0-canary.14",
    "matter-js": "^0.14.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "splitting": "^1.0.6",
    "toast": "^0.3.30"
  },
  "snowpack": {
    "exclude": [
      "public/**",
      "commonjs-compat/**",
      "toast.js"
    ],
    "installOptions": {
      "alias": {
        "react": "preact/compat"
      }
    },
    "knownEntrypoints": [
      "preact",
      "preact/hooks",
      "splitting/dist/splitting.css"
    ]
  },
  "devDependencies": {
    "http-server": "^0.12.3"
  }
}
