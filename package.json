{
  "type": "module",
  "name": "@lwj/scenes",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jason Lengstorf <jason@lengstorf.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "patch-package",
    "dev": "rm -rf .cache && yarn build && http-server public",
    "build": "toast incremental .",
    "prepare": "breadbox --dest public/web_modules",
    "breadbox": "breadbox --dest public/web_modules"
  },
  "dependencies": {
    "@sector/breadbox": "^0.0.4",
    "@socket-studio/preact": "^1.0.0-canary.3",
    "@urql/preact": "^1.3.0",
    "@xstate/react": "^1.0.2",
    "graphql": "^15.3.0",
    "isomorphic-fetch": "^3.0.0",
    "isomorphic-ws": "^4.0.1",
    "patch-package": "^6.2.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "splitting": "^1.0.6",
    "subscriptions-transport-ws": "^0.9.18",
    "toast": "^0.3.30",
    "urql": "^1.10.1",
    "ws": "^7.3.1",
    "xstate": "^4.13.0"
  },
  "snowpack": {
    "exclude": [
      "public/**",
      "commonjs-compat/**",
      "toast.js"
    ],
    "installOptions": {
      "alias": {
        "react": "preact/compat"
      }
    },
    "knownEntrypoints": [
      "preact",
      "preact/hooks",
      "splitting/dist/splitting.css"
    ]
  },
  "devDependencies": {
    "http-server": "^0.12.3",
    "serve": "^11.3.2"
  }
}
