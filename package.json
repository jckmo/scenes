{
  "type": "module",
  "name": "@lwj/scenes",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jason Lengstorf <jason@lengstorf.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "breadbox --dest public/web_modules",
    "build": "toast incremental .",
    "dev": "npm run build && http-server public",
    "serve": "http-server public"
  },
  "dependencies": {
    "@sector/breadbox": "^0.0.4",
    "@socket-studio/preact": "^1.0.0-canary.14",
    "@xstate/fsm": "^1.5.2",
    "@xstate/react": "^1.2.2",
    "matter-js": "^0.15.0",
    "netlify-plugin-toast-cache": "0.0.4",
    "poly-decomp": "^0.3.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "rehype": "^11.0.0",
    "rehype-sanitize": "^4.0.0",
    "splitting": "^1.0.6",
    "toast": "^0.3.39",
    "xstate": "^4.15.3"
  },
  "snowpack": {
    "exclude": [
      "public/**",
      "commonjs-compat/**",
      "toast.js"
    ],
    "installOptions": {
      "alias": {
        "react": "preact/compat"
      }
    },
    "knownEntrypoints": [
      "preact",
      "preact/hooks",
      "splitting",
      "splitting/dist/splitting.css"
    ]
  },
  "devDependencies": {
    "http-server": "^0.12.3"
  }
}
